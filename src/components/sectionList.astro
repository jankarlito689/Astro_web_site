---
//imports
import "../styles/components/sectionList.css"

//interface list section
interface Props{
    title: string;
    items: string[];
}

const {title, items} = Astro.props;
---

<section class="section-list">
    <h2>{title}</h2>
    <ul>
        {items.map((item) => <li>{item}</li>)}
    </ul>
</section>